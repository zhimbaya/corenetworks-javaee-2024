package service;

public class Calculadora {
	// atributos
	private int num1;
	private int num2;

	// constructor
	public Calculadora(int num1, int num2) {
		// super();
		this.num1 = num1;
		// this.num2 = num2;
	}

	// mÃ©todos con las operaciones a realizar
	public int sumar() {
		return num1 + num2;
	}

	public int restar() {
		return num1 - num2;
	}

	public int multiplicar() {
		return num1 * num2;
	}

	public int dividir() {
		return num1 / num2;
	}
}
