package principal;

import java.text.SimpleDateFormat;
import java.util.Date;

public class Test {

	public static void main(String[] args) {
		Date d1 = new Date();
		Date d2 = new Date(2337484972132l);
		if (d1.before(d2)) {
			System.out.print("La fecha m치s antigua es: " + d1);
			System.out.print("\nLa fecha m치s nueva es; " + d2);
		}else {
			System.out.print("La fecha m치s moderna es: " + d2);
			System.out.print("\nLa fecha m치s vieja es: " + d1);
		}
		//formatear fechas
		SimpleDateFormat format = new SimpleDateFormat("dd/mm/yyyy");
		String date = format(d1);
		
		
	
	}

}
